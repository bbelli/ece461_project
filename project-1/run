install () {
	python3 -m venv env/
	source ../env/bin/activate
	pip3 install -r requirements.txt
}

test () {
	source ../env/bin/activate
	python3 test.py
}

if [ "$1" == "" ]
then
	echo "ERROR No input"

elif [ $1 == "install" ]
then
	install

elif [ $1 == "test" ]
then
	test

else
	source ../env/bin/activate
	python3 proj1.py $1
fi
